//You are given an array of n elements and an integer k, you need to rotate the array by k units towards right direction

function rotate(n,t,arr){
 
 t = t % n;
 
 var res=[]
 
    for (let i = 0; i < n; i++) {
        if (i < t) {
            res.push(arr[n + i - t]);
        }
        else {

            res.push((arr[i - t]));
        }
    }
  
  
      console.log(res.join(" "))
    
}
